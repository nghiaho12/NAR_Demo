@INPROCEEDINGS{REVIEW_SEXTANT,
  author = {Cozman, F. and Krotkov, E.},
  title = {Robot localization using a computer vision sextant},
  booktitle = {Robotics and Automation, 1995. Proceedings., 1995 IEEE International
	Conference on},
  year = {1995},
  volume = {1},
  pages = {106-111 vol.1},
  month = {May},
  abstract = {This paper explores the possibility of using Sun altitude for localization
	of a robot in totally unknown territory. A set of Sun altitudes is
	obtained by processing a sequence of time-indexed images of the sky.
	Each altitude constrains the viewer to a circle on the surface of
	a celestial body, called the circle of equal altitude. A set of circles
	of equal altitude can be intersected to yield viewer position. We
	use this principle to obtain the position on Earth. Since altitude
	measurements are corrupted by noise, a least-square estimate is numerically
	calculated from the sequence of altitudes. The paper discusses the
	necessary theory for Sun-based localization, the technical issues
	of camera calibration and image processing, and presents preliminary
	results with real data},
  doi = {10.1109/ROBOT.1995.525271},
  issn = {1050-4729},
